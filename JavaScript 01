var soma, n1, n2, n3, n4, n5

//Solicita os cinco números ao user
n1 = parseInt(prompt("Informe o primeiro número: "))
n2 = parseInt(prompt("Informe o segundo número: "))
n3 = parseInt(prompt("Informe o terceiro número: "))
n4 = parseInt(prompt("Informe o quarto número: "))
n5 = parseInt(prompt("Informe o quinto número: "))

//Soma dos cinco números
soma = n1+n2+n3+n4+n5

//Mostra o resultado da soma
window.alert("A soma dos número é " + soma)
